{
    "compilerOptions": {
        "declaration": false,
        "esModuleInterop": true,
        "lib": ["ES6"],
        "module": "CommonJS",
        "moduleResolution": "node",
        "newLine": "lf",
        "noEmitOnError": true,
        "noUnusedLocals": true,
        "pretty": true,
        "resolveJsonModule": true,
        "stripInternal": true,
        "target": "ES6",
    },
    "include": [
        "src",
        "webpack.config.ts"
    ]
}