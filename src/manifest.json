{
    "name": "Stevens Web Extension",
    "version": "0.0.0",
    "manifest_version": 2,
    "permissions": ["activeTab", "storage", "contextMenus", "bookmarks"],
    "icons": {
        "128": "assets/temp_icon.png"
    },
    "browser_action": {
        "default_icon": "assets/temp_icon.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}
