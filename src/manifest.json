{
    "name": "Stevens Web Extension",
    "version": "0.0.0",
    "manifest_version": 2,
    "permissions": [
        "activeTab",
        "alarms",
        "bookmarks",
        "contextMenus",
        "storage",
        "<all_urls>"
    ],
    "icons": {
        "128": "assets/temp_icon.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "options_ui": {
        "page": "options.html",
        "chrome_style": true,
        "open_in_tab": true
    }
}
